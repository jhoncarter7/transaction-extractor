# Backend Environment Variables
# Copy this file to .env and fill in the values

# Database Connection (required)
DATABASE_URL="postgresql://user:password@localhost:5432/transaction_extractor?schema=public"

# Better Auth Configuration (required)
# Secret key for signing sessions (minimum 32 characters)
BETTER_AUTH_SECRET="your-super-secret-key-at-least-32-characters-long"

# Base URL of the backend server
BETTER_AUTH_URL="http://localhost:3000"

# Server Configuration
PORT=3000
NODE_ENV=development

# Frontend URL for CORS
FRONTEND_URL="http://localhost:3001"
